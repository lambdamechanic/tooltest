#!/usr/bin/env sh

set -u

base_ref=""
head_ref=""

if [ "$#" -ge 2 ]; then
    base_ref=$1
    head_ref=$2
elif git rev-parse -q --verify HEAD@{1} >/dev/null 2>&1; then
    base_ref="HEAD@{1}"
    head_ref="HEAD"
fi

if [ -z "$base_ref" ] || [ -z "$head_ref" ]; then
    exit 0
fi

"$(dirname "$0")/_br_sync_import.sh" "checkout" "$base_ref" "$head_ref"
